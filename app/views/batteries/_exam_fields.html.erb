<fieldset>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <% es = ExamSelection.find_by(exam: f.object, battery: @battery) %>
  <%= f.fields_for :exam_selection, es do |builder| %>
    <%= builder.label :order %>
    <%= builder.text_field :order %>

    <%= builder.hidden_field :exam_id %>
    <%= builder.hidden_field :battery_id %>
  <% end %>

  <%= f.hidden_field :_destroy %>
  <br>
  <%= link_to 'Eliminar', '#', class: 'remove_fields' %>
</fieldset>