<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<% @users.each do |user| %>
  <h3><%= user.email %></h3>
  <ul>
    <li>Rol: <%= user.role.name %></li>
    <%= render "users/profile_infos/#{User.parse_profile_type(user.profile_type)}_info", user: user %>
  </ul>
<% end %>

<br>

